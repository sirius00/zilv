view{padding:0;margin:0;box-sizing:border-box}.tabbar-container{position:fixed;bottom:0rpx;left:0rpx;width:100%;height:110rpx;box-shadow:0 0 5px #999;display:flex;align-items:center;padding:5rpx 0;color:#999;background-color:#fff}.tabbar-container .tabbar-item{width:33.33%;height:100rpx;display:flex;flex-direction:column;justify-content:center;align-items:center;text-align:center}.tabbar-container .tabbar-item .item-top{width:70rpx;height:70rpx;padding:10rpx}.tabbar-container .tabbar-item .item-top image{width:100%;height:100%}.tabbar-container .tabbar-item .item-bottom{font-size:28rpx;width:100%}.tabbar-container .tabbar-item .item-active{color:red}.tabbar-container .center-item{display:block;position:relative}.tabbar-container .center-item .item-top{flex-shrink:0;width:150rpx;height:150rpx;position:absolute;top:-65rpx;left:calc(50% - 70rpx)}.tabbar-container .center-item .item-bottom{position:absolute;bottom:5rpx}.tabbar-container .center-item .item-active{position:absolute;bottom:5rpx;color:#1ff}